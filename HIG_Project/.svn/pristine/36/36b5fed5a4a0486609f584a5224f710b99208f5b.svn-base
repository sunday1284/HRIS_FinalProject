document.getElementById("messengerBtn").addEventListener("click",function() {
	let contextPath = window.contextPath;
	let url = contextPath+"/messenger/Main";
	const popupOptions = "width=600,height=500,left=300,top=100,resizable=yes,scrollbars=yes";
	window.open(
		  url
		, "MessengerPopup"
		, popupOptions
	);
});